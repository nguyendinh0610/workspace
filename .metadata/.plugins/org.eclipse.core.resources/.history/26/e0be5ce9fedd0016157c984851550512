package LinkedList;

import java.util.*;

public class FindKthToLastElement {
	public static void main(String[] args){
		
	}
	
	class Node{
		int data;
		Node next = null;
		
		public Node(int value){
			data = value;
		}
		
		public void appendToEnd(int d){
			Node end = new Node(d);
			Node n = this;
			while (n.next != null){
				n = n.next;
			}
			n.next = end;
		}
	}
}
