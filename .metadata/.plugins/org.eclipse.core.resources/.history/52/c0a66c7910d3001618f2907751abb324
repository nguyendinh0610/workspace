package ArraysAndStrings;

import java.util.*;

public class PictureRotation {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		sc.nextLine();
		int[][] pic = new int[n][n];
		for (int i = 0; i < n; i++){
			for (int j = 0; j < n; j++){
				pic[i][j] = sc.nextInt();
			}
			sc.nextLine();
		}
		sc.close();
		for (int i = 0; i < n; i++){
			for (int j = 0; j < n; j++){
				System.out.print(pic[i][j]);
			}
			System.out.println();
		}
	}
}
