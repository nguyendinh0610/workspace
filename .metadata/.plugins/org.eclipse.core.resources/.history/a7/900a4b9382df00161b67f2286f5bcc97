package LinkedList;

import java.util.*;

public class LinkedListPartitioning {
	public static void main(String[] args){
		
	}
	
	class Node{
		int data;
		Node next = null;
		
		public Node(int value){
			data = value;
		}
		
		public void appendToEnd(int d){
			Node current = this;
			Node end = new Node(d);
			while (current.next != null){
				current = current.next;
			}
			current.next = end;
		}
		
		public void appendToFront(Node head, int d){
			Node front = new Node(d);
			front.next = head;
		}
		
		public void partitionLinkedList(Node head, int i){
			if (head == null){
				return;
			}
			
			Node smallerHead;
			Node biggerHead;
			
			while (head.next != null){
				
			}
			
		}
	}
}
